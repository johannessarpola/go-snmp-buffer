    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Listener",
            "cwd": "${workspaceFolder}",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/appcli/main.go",
            "args": ["listen"]
        },
        {
            "name": "Launch Producer",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/snmp-producer-v2/main.go"
        },
        {
            "name": "Launch Forwarder",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/snmp-forwarder/main.go"
        },
        {
            "name": "Launch Admin CLI",
            "cwd": "${workspaceFolder}",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/admincli/main.go"
        },
        {
            "name": "Launch Admin CLI [list indexes]",
            "cwd": "${workspaceFolder}",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/admincli/main.go",
            "args": ["index", "list"]
        },
        {
            "name": "Launch Admin CLI [get index]",
            "cwd": "${workspaceFolder}",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/admincli/main.go",
            "args": ["index", "g", "-i", "current_idx"]
        },
        {
            "name": "Launch Admin CLI [get offset_idx]",
            "cwd": "${workspaceFolder}",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/admincli/main.go",
            "args": ["index", "g", "-i", "offset_idx"]
        }
    ]
}